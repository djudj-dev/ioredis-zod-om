{
  "name": "@e2fy/ioredis-zod-om",
  "version": "1.0.0",
  "description": "A library for create typesafe schema for redis with ioRedis and Zod",
  "homepage": "https://github.com/djudj-dev/ioredis-zod-om",
  "repository": {
    "type": "git",
    "url": "https://github.com/djudj-dev/ioredis-zod-om"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "start:watch": "ts-node-dev src/index.ts",
    "start:build": "node dist/index.js",
    "test": "vitest --coverage",
    "test:ui": "vitest --ui --coverage",
    "build": "tsc",
    "publish:npm": "vitest --run && pnpm build && npm publish --access public"
  },
  "keywords": [
    "redis",
    "zod",
    "ioredis"
  ],
  "author": "@Djudj_dev",
  "license": "MIT",
  "dependencies": {
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "ioredis": "^5.3.2",
    "ioredis-mock": "^8.7.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.3",
    "vitest": "^0.32.2"
  }
}
